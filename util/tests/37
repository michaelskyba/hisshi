p $HISSHI_VAR
date +%Y | head -c 1 |= HISSHI_VAR
p $HISSHI_VAR

# Should initially be empty
env | grep HISSHI_VAR
- echo HISSHI_VAR not found

export HISSHI_VAR

# Should be the same
p $HISSHI_VAR

# Should exist now
env | grep "^HISSHI_VAR="

export HISSHI_OTHER_VAR

# Still shouldn't exist
env | grep "^HISSHI_OTHER_VAR="
- echo doesnt

# Both should fail. Both are supported in POSIX but not us. See builtin.c or
# other docs for design decision explanations
export
export foo bar
