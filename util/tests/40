test $var
- echo "Empty initially"

# Runs in the parent process so var should be visible
eval "pwd |= var"
test $var
	p OK
-
	p FAIL

# Pipelines run each command in child processes so the inner eval will have a
# dangling copy of the parent shell_state
eval "pwd |= var2" | cat
test $var2
	p FAIL
-
	p OK
