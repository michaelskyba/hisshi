echo /tmp/hisshi-tmp.95SQiVZinp |= dir
mkdir $dir

pwd | eq $dir
	echo FAIL
	exit 1
-
	echo "OK: dir not matching initially"

cd $dir

pwd | eq $dir
	echo "OK: regular cd worked"
-
	echo FAIL
	exit 1

cd / | cat

pwd | eq $dir
	echo "OK: child cd did nothing"
-
	echo FAIL
	exit 1

pwd
seq 10 | rw 3 |= bar
echo $bar

cd /
rmdir $dir
